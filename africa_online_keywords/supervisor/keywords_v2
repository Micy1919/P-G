[program:keyword_v2_<language>]
# put server.crt and server.key in this directory
directory=<install_dir>
# use "sleep 60" as supervisor doesn't support delay-between-retries.
environment=PYTHONPATH=<install_dir>
command=python keyword_server.py --port=<port>
startsecs=60
startretries=100
autostart=true
autorestart=true
stderr_logfile=/<install_dir>/log/%(program_name)s.err.log
stderr_logfile_maxbytes=50MB
stdout_logfile=/<install_dir>/log/%(program_name)s.out.log
stdout_logfile_maxbytes=50MB