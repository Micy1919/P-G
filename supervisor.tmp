[program:server_1]
# put server.crt and server.key in this directory
directory=<install_dir>
# use "sleep 60" as supervisor doesn't support delay-between-retries.
#environment=PYTHONPATH=/mnt/supervised_keywords
command=bash -c "exec python server.py --lang=<language> --port=<port_1>"
startsecs=60
startretries=100
autostart=true
autorestart=true
stderr_logfile=/mnt/log/%(program_name)s.err.log
stderr_logfile_maxbytes=50MB
stdout_logfile=/mnt/log/%(program_name)s.out.log
stdout_logfile_maxbytes=50MB

[program:server_2]
# put server.crt and server.key in this directory
directory=<install_dir>
# use "sleep 60" as supervisor doesn't support delay-between-retries.
#environment=PYTHONPATH=/mnt/supervised_keywords
command=bash -c "exec python server.py --lang=<language> --port=<port_2>"
startsecs=60
startretries=100
autostart=true
autorestart=true
stderr_logfile=/mnt/log/%(program_name)s.err.log
stderr_logfile_maxbytes=50MB
stdout_logfile=/mnt/log/%(program_name)s.out.log
stdout_logfile_maxbytes=50MB

[group:supervised_keywords_server_<country>]
programs = server_1, server_2
