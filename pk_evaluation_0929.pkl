V \u0627\u0633\u0644\u0627\u0645 \u0627\u0653\u0628\u0627\u062f (\u0627\u064f\u0631\u062f\u0648 \u067e\u0648\u0627\u0626\u0646\u0679 \u0627\u062e\u0628\u0627\u0631\u062a\u0627\u0632\u06c1 \u062a\u0631\u06cc\u0646\u06d4 28 \u0633\u062a\u0645\u0628\u06312017\u0621)\u0642\u0627\u0626\u0645\u0642\u0627\u0645 \u0633\u067e\u06cc\u06a9\u0631 \u0642\u0648\u0645\u06cc \u0627\u0633\u0645\u0628\u0644\u06cc \u0645\u0631\u062a\u0636\u06cc\u0670 \u062c\u0627\u0648\u06cc\u062f \u0639\u0628\u0627\u0633\u06cc \u0646\u06d2 \u06a9\u06c1\u0627 \u06c1\u06d2 \u06a9\u06c1 \u0645\u0648\u062c\u0648\u062f\u06c1 \u062d\u06a9\u0648\u0645\u062a \u0645\u0644\u06a9 \u0633\u06d2 \u067e\u0633\u0645\u0627\u0646\u062f\u06af\u06cc \u0627\u0648\u0631 \u0645\u0644\u06a9 \u06a9\u06d2 \u062f\u0648\u0631 \u062f\u0631\u0627\u0632 \u0639\u0644\u0627\u0642\u0648\u06ba \u06a9\u06cc \u062a\u0631\u0642\u06cc \u067e\u0631 \u062e\u0635\u0648\u0635\u06cc \u062a\u0648\u062c\u06c1 \u062f\u06d2 \u0631\u06c1\u06cc \u06c1\u06d2 \u06d4 \u0627\u0646\u06c1\u0648\u06ba \u06a9\u06c1\u0627 \u06a9\u06c1 \u0645\u0633\u0644\u0645 \u0644\u06cc\u06af (\u0646)\u06a9\u06cc \u0645\u0648\u062c\u0648\u062f\u06c1 \u062d\u06a9\u0648\u0645\u062a \u0646\u06d2 \u06c1\u0632\u0627\u0631\u06c1 \u0688\u0648\u06cc\u0698\u0646 \u0645\u06cc\u06ba \u0631\u06cc\u06a9\u0627\u0631\u0688 \u062a\u0631\u0642\u06cc\u0627\u062a\u06cc \u06a9\u0627\u0645 \u06a9\u0631\u0648\u0627\u0626\u06d2 \u06c1\u06cc\u06ba\u06d4\u0627\u0646\u06c1\u0648\u06ba \u0646\u06d2 \u06a9\u06c1\u0627 \u06a9\u06c1 \u0646\u0648\u0627\u0632 \u0634\u0631\u06cc\u0641 \u0633\u0628 \u0633\u06d2 \u0632\u06cc\u0627\u062f\u06c1 \u0645\u0642\u0628\u0648\u0644 \u0644\u06cc\u0688\u0631\u06c1\u06cc\u06ba\u0627\u0648\u0631 \u0639\u0648\u0627\u0645 \u06a9\u06d2 \u062f\u0644\u0648\u06ba \u0645\u06cc\u06ba \u0628\u0633\u062a\u06d2 \u06c1\u06cc\u06ba \u0645\u062e\u0627\u0644\u0641\u06cc\u0646 \u06a9\u06d2 \u0627\u064f\u0646 \u06a9\u06d2 \u062e\u0644\u0627\u0641 \u0628\u06d2 \u0628\u0646\u06cc\u0627\u062f\u0627\u0648\u0631\u062c\u06be\u0648\u0679\u06d2 \u067e\u0631\u0648\u067e\u06cc\u06af\u0646\u0688\u06d2 \u0627\u064f\u0646\u06c1\u06cc\u06ba \u0639\u0648\u0627\u0645 \u06a9\u06d2 \u062f\u0644\u0648\u06ba \u0633\u06d2 \u0646\u06c1\u06cc\u06ba \u0646\u06a9\u0627\u0644 \u0633\u06a9\u062a\u06d2 \u06d4_line_ \u0627\u0646\u06c1\u0648\u06ba \u0646\u06d2 \u0627\u0646 \u062e\u06cc\u0627\u0644\u0627\u062a \u06a9\u0627 \u0627\u0638\u06c1\u0627\u0631 \u062d\u0648\u06cc\u0644\u06cc\u0627\u06ba \u06a9\u06d2 \u0646\u0648\u0627\u0626\u06cc \u0639\u0644\u0627\u0642\u06d2 \u0633\u0631\u06cc\u0644\u06c1\u060c \u0628\u0679\u0648\u0644\u0646\u06cc \u0645\u06cc\u06ba \u0627\u06cc\u06a9 \u0628\u0691\u06d2 \u0639\u0648\u0627\u0645\u06cc \u0627\u062c\u062a\u0645\u0627\u0639 \u0633\u06d2 \u062e\u0637\u0627\u0628 \u06a9\u0631\u062a\u06d2 \u06c1\u0648\u0626\u06d2 \u06a9\u06cc\u0627\u06d4\u0642\u0627\u0626\u0645\u0642\u0627\u0645 \u0633\u067e\u06cc\u06a9\u0631 \u0646\u06d2 \u06a9\u06c1\u0627 \u06a9\u06c1 \u0645\u0644\u06a9 \u06a9\u06d2 \u0637\u0648\u0644 \u0648 \u0639\u0631\u0636 \u0645\u06cc\u06ba\u0627\u0631\u0628\u0648\u06ba \u0631\u0648\u067e\u06d2 \u06a9\u06d2 \u062a\u0631\u0642\u06cc\u0627\u062a\u06cc \u0645\u0646\u0635\u0648\u0628\u0648\u06ba \u067e\u0631 \u06a9\u0627\u0645 \u062c\u0627\u0631\u06cc \u06c1\u06d2 \u062c\u0646 \u06a9\u06cc \u062a\u06a9\u0645\u06cc\u0644 \u0633\u06d2 \u0645\u0644\u06a9 \u0645\u06cc\u06ba \u062a\u0631\u0642\u06cc \u0648 \u062e\u0648\u0634\u062d\u0627\u0644\u06cc \u06a9\u06d2 \u0627\u06cc\u06a9 \u0646\u0626\u06d2 \u062f\u0648\u0631 \u06a9\u0627 \u0627\u0653\u063a\u0627\u0632 \u06c1\u0648 \u06af\u0627\u06d4 \u0627\u0646\u06c1\u0648\u06ba \u0646\u06d2 \u06a9\u06c1\u0627 \u06a9\u06c1 2018\u0621 \u06a9\u06d2 \u0627\u0644\u06cc\u06a9\u0634\u0646 \u0645\u06cc\u06ba \u06a9\u0627\u0631\u06a9\u0631\u062f\u06af\u06cc \u06a9\u06cc \u0628\u0646\u06cc\u0627\u062f \u067e\u0631 \u06a9\u0627\u0645\u06cc\u0627\u0628\u06cc \u062d\u0627\u0635\u0644 \u06a9\u0631\u06cc\u06ba \u06af\u06d2 \u0627\u0648\u0631 \u0645\u0631\u06a9\u0632 \u0633\u0645\u06cc\u062a \u0686\u0627\u0631\u0648\u06ba \u0635\u0648\u0628\u0648\u06ba \u0645\u06cc\u06ba\u067e\u0627\u06a9\u0633\u062a\u0627\u0646 \u0645\u0633\u0644\u0645 \u0644\u06cc\u06af (\u0646) \u062d\u06a9\u0648\u0645\u062a \u0628\u0646\u0627\u0626\u06d2 \u06af\u06cc\u06d4_line_ \u0627\u0633 \u0645\u0648\u0642\u0639 \u067e\u0631 \u0627\u0646\u06c1\u0648\u06ba \u0646\u06d2 \u06af\u0648\u0631\u0646\u0645\u0646\u0679 \u0645\u0688\u0644 \u0633\u06a9\u0648\u0644 \u0633\u0631\u06cc\u0644\u06c1 \u06a9\u0648 \u0627\u067e \u06af\u0631\u06cc\u0688 \u06a9\u0631 \u06a9\u06d2 \u06c1\u0627\u0626\u06cc \u0633\u06a9\u0648\u0644 \u06a9\u0627 \u062f\u0631\u062c\u06c1 \u062f\u06cc\u0646\u06d2 \u06a9\u0627 \u0627\u0639\u0644\u0627\u0646 \u0628\u06be\u06cc \u06a9\u06cc\u0627 \u062c\u0633 \u067e\u0631 2\u06a9\u0631\u0648\u0691 13\u0644\u0627\u06a9\u06be \u0631\u0648\u067e\u06d2 \u06a9\u06cc \u0644\u0627\u06af\u062a \u0627\u0653\u0626\u06d2 \u06af\u06cc \u06d4\u0627\u0646\u06c1\u0648\u06ba\u0646\u06d2 \u06a9\u06c1\u0627 \u06a9\u06c1 \u0635\u0648\u0628\u0627\u0626\u06cc \u062d\u06a9\u0648\u0645\u062a \u06c1\u0645\u0627\u0631\u06d2 \u0645\u0646\u0635\u0648\u0628\u0648\u06ba \u0645\u06cc\u06ba \u0631\u0648\u0691\u06d2 \u0627\u0679\u06a9\u0627\u0631\u06c1\u06cc \u06c1\u06d2 \u062a\u0627\u06c1\u0645 \u0627\u0633 \u06a9\u06c1 \u0628\u0627\u0648\u062c\u0648\u062f \u0627 \u062a\u0645\u0627\u0645 \u062a\u0631\u0642\u06cc\u0627\u062a\u06cc \u0645\u0646\u0635\u0648\u0628\u0648\u06d2 \u0628\u0631\u0648\u0642\u062a \u0645\u06a9\u0645\u0644 \u06a9\u06cc\u06d2 \u062c\u0627\u0626\u06cc\u06ba\u06af\u06d2\u06d4 \u0627\u0646\u06c1\u0648\u06ba \u0646\u06d2 \u062a\u062d\u0635\u06cc\u0644 \u062d\u0648\u06cc\u0644\u06cc\u0627\u06ba \u06a9\u06cc \u06cc\u0648\u0646\u06cc\u0646 \u06a9\u0648\u0646\u0633\u0644\u0648\u06ba \u0633\u0644\u0648\u0627\u0644\u06c1\u060c \u062a\u0631\u0645\u0648\u0686\u06be\u06cc\u0627\u06ba \u060c\u0633\u0631\u06cc\u0644\u06c1 \u0627\u0648\u0631 \u0628\u0679\u0648\u0644\u0646\u06cc \u0645\u06cc\u06ba \u06af\u06cc\u0633 \u0641\u0631\u0627\u06c1\u0645\u06cc \u06a9\u06d2 \u0645\u0646\u0635\u0648\u0628\u0648\u06ba \u06a9\u0627 \u0630\u06a9\u0631 \u06a9\u0631\u062a\u06d2 \u06c1\u0648\u0626\u06d2 \u06a9\u06c1\u0627 \u06a9\u06c1 \u0627\u0646 \u0686\u0627\u0631 \u06cc\u0648\u0646\u06cc\u0646 \u06a9\u0648\u0646\u0633\u0644\u0648\u06ba \u0645\u06cc\u06ba \u06af\u06cc\u0633 \u06a9\u06cc \u0641\u0631\u0627\u06c1\u0645\u06cc \u06a9\u06d2 \u0644\u06cc\u06d2 19\u06a9\u0631\u0648\u0691 \u0631\u0648\u067e\u06d2 \u06a9\u06cc \u0644\u0627\u06af\u062a \u0633\u06d2 \u0633\u0648\u0626\u06cc \u06af\u06cc\u0633 \u0641\u0631\u0627\u06c1\u0645 \u06a9\u06cc \u062c\u0627\u0631\u06c1\u06cc \u06c1\u06d2 \u06d4_line_\u0627\u0646\u06c1\u0648\u06ba \u0627\u0633 \u0627\u0639\u062a\u0645\u0627\u062f \u06a9\u0627 \u0627\u0638\u06c1\u0627\u0631 \u06a9\u06cc\u0627 \u06a9\u06c1 \u0627\u0653\u0626\u0646\u062f\u06c1 \u0627\u0644\u06cc\u06a9\u0634\u0646 \u0633\u06d2 \u0642\u0628\u0644 \u0627\u0646 \u0686\u0627\u0631 \u06cc\u0648\u0646\u06cc\u0646 \u06a9\u0648\u0646\u0633\u0644\u0648\u06ba \u0645\u06cc\u06ba \u06af\u06cc\u0633 \u0641\u0631\u0627\u06c1\u0645\u06cc \u06a9\u06d2 \u0645\u0646\u0635\u0648\u0628\u06d2 \u0645\u06a9\u0645\u0644 \u06a9\u0631 \u0644\u06cc\u06d2 \u062c\u0627\u0626\u06cc\u06ba \u06af\u06d2 \u06d4\u0627\u0646\u06c1\u0648\u06ba \u0646\u06d2 \u06a9\u0633\u06cc\u0644\u0627\u06ba \u062a\u0627 \u0628\u0679\u0648\u0644\u0646\u06cc \u0633\u0691\u06a9 \u067e\u0631 \u06a9\u0627\u0645 \u062c\u0644\u062f \u0634\u0631\u0648\u0639 \u06a9\u0631\u0646\u06d2 \u06a9\u0627 \u0628\u06be\u06cc \u0627\u0639\u0644\u0627\u0646 \u06a9\u06cc\u0627 \u06d4\u0645\u0631\u062a\u0636\u06cc \u062c\u0627\u0648\u06cc\u062f \u0639\u0628\u0627\u0633\u06cc \u0646\u06d2 \u06a9\u06c1\u0627 \u06a9\u06c1 \u062e\u06cc\u0628\u0631 \u067e\u062e\u062a\u0648\u0646 \u062e\u0648\u0627\u06c1 \u0645\u06cc\u06ba \u067e\u0627\u06a9\u0633\u062a\u0627\u0646 \u06